{
    "manifest_version": 3,
    "name": "JSON to Dart Converter",
    "version": "1.0",
    "description": "Convert JSON to Dart model class.",
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/icon16.png",
            "48": "icons/icon48.png",
            "128": "icons/icon128.png"
        }
    },
    "permissions": [
        "storage",
        "activeTab"
    ],
    "background": {
        "service_worker": "background.js"
    }
}